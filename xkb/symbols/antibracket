	};
	key <AD12> {
		[ XF86Explorer,             dead_tilde,              dead_stroke,             dead_psili              ],
		[ NoSymbol,                NoSymbol                  ]
	};

	// Middle row
	// --------------------------------------------------------------
	key <CAPS> {
		type[Group1] = "ONE_LEVEL",
		symbols[Group1] = [ Tab ],
		type[Group2] = "ONE_LEVEL",
		symbols[Group2] = [ Multi_key ]
	};

	key.type[Group1] = "FOUR_LEVEL_SEMIALPHABETIC";
	key <AC01> {
		[ l,                       L,                       slash,                   NoSymbol                ],
