// vim: se ft=xkb :
partial alphanumeric_keys modifier_keys keypad_keys
xkb_symbols "basic" {

	// Alphanumeric-keys
	// ===============
	key.type[Group1] = "FOUR_LEVEL";
	key.type[Group2] = "TWO_LEVEL";

	// Tab as Multi_key (Compose)
	// --------------------------------------------------------------
	key  <TAB> {
		[ 0xfd12,               0xfd12,               Multi_key,               ISO_Level5_Lock         ],
		[ Multi_key,               NoSymbol                  ]
	};


	// Number row
	// --------------------------------------------------------------
	key <TLDE> {
		[ grave,                   asciitilde,              U21BB,                   U02DE                   ],
		[ NoSymbol,                NoSymbol                  ]
	};

